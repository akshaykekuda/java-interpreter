int x=1;
while (x<0) {
    float x = 1.2;
    float y = x-1.;
    if (y>1.2){
      int y = 1;
      float x = -2.88;  
      if (y<1){
          float y = -1.2;
          y = x+y;
          
      }
      else{
          float y = -x;
          int x = -1;
      }
    } 
    else {
        y=y+x;
    }
}
